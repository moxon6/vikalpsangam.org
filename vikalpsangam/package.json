{
  "name": "underscores",
  "version": "1.0.0",
  "description": "http://vikalpsangam.org Theme",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme",
    "http://vikalpsangam.org/",
    "vikalpsangam"
  ],
  "homepage": "http://vikalpsangam.org/",
  "devDependencies": {
    "@wordpress/scripts": "^12.1.0",
    "dir-archiver": "^1.1.1",
    "node-sass": "^4.14.1",
    "rtlcss": "^2.5.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "watch:scss": "node-sass -o . sass/style.scss -w",
    "build:scss": "node-sass -o . sass/style.scss",
    "build:scss-rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style sass/**/*.scss",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../vikalpsangam.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "db:migrate-dump": "php ../tools/migrate-database/pg2mysql_cli.php /workspace/db.sql /workspace/dump-mysql.sql",
    "db:create-new": "MYSQL_PWD=somewordpress mysql -h db -u root < /workspace/tools/create-new-db.sql",
    "db:upload-mysql-dump": "MYSQL_PWD=somewordpress mysql -h db -u root public < /workspace/dump-mysql.sql",
    "sync:media": "rsync -a /mnt/c/dev/vikalp-wordpress/uploads/ ubuntu@vikalp:/var/www/html/wp-content/uploads",
    "recount-taxonomies": "wp taxonomy --path=/var/www/html list --field=name --allow-root | xargs wp --path=/var/www/html term recount --allow-root"
  }
}
