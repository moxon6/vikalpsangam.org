{
  "name": "new-migration-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sequelize:generate_models:django": "sequelize-auto -o \"./src/django/models\" -d main -h postgres -u postgres -p 5432 -x postgres -e postgres",
    "sequelize:generate_models:wordpress": "sequelize-auto -o \"./src/wordpress/models\" -d wordpress -h db -u wordpress -p 3306 -x wordpress -e mysql",
    "django:rebuild": "psql --username=postgres --host=postgres -f src/django/rebuild.sql",
    "django:load": "psql --username=postgres --host=postgres --dbname=main -f /workspace/db.postgres.sql",
    "django:export": "babel-node src/django/export-posts.js",
    
    "validate": "babel-node src/validate/validate-posts-json.js",
    
    "wordpress:reset-content": "babel-node src/wordpress/reset-wordpress-content.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "joi": "^17.2.1",
    "mysql2": "^2.2.5",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.1",
    "sequelize": "^6.3.5",
    "sequelize-auto": "^0.6.4"
  }
}
