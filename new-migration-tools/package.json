{
  "name": "new-migration-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "rebuild_db": "psql --username=postgres --host=postgres -f rebuild.sql",
    "load_db": "psql --username=postgres --host=postgres --dbname=main -f /workspace/db.postgres.sql",
    "patch_columns": "psql --username=postgres --host=postgres --dbname=main -f rename-column.sql",
    "generate_models:django": "sequelize-auto -o \"./src/models/django\" -d main -h postgres -u postgres -p 5432 -x postgres -e postgres",
    "generate_models:wordpress": "sequelize-auto -o \"./src/models/wordpress\" -d wordpress -h db -u wordpress -p 3306 -x wordpress -e mysql",
    "start": "babel-node src/export.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "cheerio": "^1.0.0-rc.3",
    "mysql2": "^2.2.5",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.1",
    "sequelize": "^6.3.5",
    "sequelize-auto": "^0.6.4"
  }
}
